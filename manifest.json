{
  "manifest_version": 2,
  "name": "Histograph",
  "description": "Browser history visualization with clean tool.",
  "version": "0.1",
  "permissions": [
    "history"
  ],
  "background": {
    "scripts": ["js/vendor/angular.min.js",
                "js/vendor/angular-route.min.js",
                "js/vendor/d3.min.js"]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides" : {
    "newtab": "tab.html"
  }
}

